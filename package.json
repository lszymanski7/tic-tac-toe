{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "description": "Tic-tac-toe is a simple strategy game played by two players on a three-by-three board. Each of them aims to win by marking three squares in one line with the same symbol - X or O.",
  "author": "Łukasz Szymański",
  "license": "MIT",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "build": "webpack --config config/webpack/webpack.prod.js",
    "dev-server": "webpack serve --config config/webpack/webpack.dev.js",
    "eslint:check": "npx eslint .",
    "eslint:fix": "npx eslint --fix .",
    "heroku-postbuild": "npm run build",
    "prettier:check": "npx prettier --check .",
    "prettier:fix": "npx prettier --write .",
    "start": "node server/server.js",
    "test": "jest --config config/jest/jest.config.js",
    "test:coverage": "jest --config config/jest/jest.config.js --coverage",
    "test:watch": "jest --config config/jest/jest.config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lszymanski7/tic-tac-toe.git"
  },
  "bugs": {
    "url": "https://github.com/lszymanski7/tic-tac-toe/issues"
  },
  "homepage": "https://github.com/lszymanski7/tic-tac-toe#readme",
  "dependencies": {
    "@csstools/normalize.css": "^12.1.1",
    "express": "^4.18.2",
